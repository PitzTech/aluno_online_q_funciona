{
  "name": "aluno_online_q_funciona",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "https://github.com/PitzTech/aluno_online_q_funciona",
  "author": "Victor Laurentino <victorlaurentino7@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn ts-node-dev ./src/server.ts",
    "build": "yarn tsc",
    "postinstall": "yarn build",
    "typeorm": "typeorm-ts-node-esm",
    "migration:create": "typeorm migration:create ./db/migration/Create_User_Table",
    "migration:run": "yarn typeorm -- migration:run -d ./db/data-source.ts",
    "entity:create": "yarn typeorm entity:create ./db/entity/User"
  },
  "dependencies": {
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "pg": "8.11.5",
    "reflect-metadata": "0.2.2",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typeorm": "0.3.20",
    "typescript": "5.4.5"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.12.10"
  }
}
